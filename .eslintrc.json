{
  "root": true,
  "env": {
    "jquery": true,
    "node": true
  },
  "extends": [
    "plugin:vue/essential",
    "eslint:recommended"
  ],
  "rules": {
    "no-console": "off",
    "vue/no-use-v-if-with-v-for": "off",
    "vue/require-v-for-key": "on"
  },
  "parserOptions": {
    "parser": "babel-eslint"
  }
}
